{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "semicolon": [
      false,
      "always"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  }
}